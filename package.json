		"husky": "^1.3.1",
		"jest": "^23.6.0",
		"jest-emotion": "^9.2.7",
		"lint-staged": "^7.2.2",
		"prettier": "^1.14.2",
		"style-loader": "^0.23.0",
		"webpack": "^4.19.0",
		"webpack-bundle-analyzer": "^3.0.2",
		"webpack-cli": "^3.1.0",
