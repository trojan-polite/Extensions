{
	"name": "@appbaseio/dejavu",
	"description": "Dejavu - The missing Web UI for Elasticsearch",
	"version": "3.2.3",
	"main": "index.js",
	"repository": "https://github.com/appbaseio/dejavu.git",
	"bugs": {
		"url": "https://github.com/appbaseio/dejavu/issues"
	},
	"engines": {
		"node": ">=8",
		"yarn": ">=1.3.2"
	},
	"scripts": {
		"start": "NODE_ENV=development webpack-dev-server --open --mode development --content-base dist/app --history-api-fallback --hot --progress --colors --port 1358",
		"start-site": "NODE_ENV=development webpack-dev-server --open --mode development --content-base dist/site --history-api-fallback --hot --progress --colors --port 1359 --config webpack.config.site.js",
		"test": "jest --coverage",
		"test:watch": "jest --watch",
		"lint": "eslint .",
		"build:app": "NODE_ENV=production webpack",
		"build:site": "NODE_ENV=product webpack --config webpack.config.site.js",
		"build": "yarn build:app && yarn build:site",
		"format": "prettier --write --use-tabs 'app/src/!(batteries*)**/**/*.js'",
		"build:chrome-extension": "yarn build:app && cd dist/app && rm favicon/manifest.json _redirects && zip -r ../dejavu.zip ."
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged && yarn test && yarn flow"
		}
	},
	"lint-staged": {
		"./app/**/*.js": "eslint"
	},
	"dependencies": {
		"@appbaseio/designkit": "^0.9.1",
		"@appbaseio/reactivesearch": "^3.0.0-rc.10",
		"@divyanshu013/media": "^1.0.0",
		"antd": "^3.10.4",
		"appbase-js": "^4.0.2-beta.2",
		"brace": "^0.11.1",
		"codesandbox": "^1.3.5",
		"cross-storage": "^1.0.0",
		"dom-helpers": "^3.4.0",
		"emotion": "^9.2.12",
		"file-saver": "^2.0.0-rc.4",
		"flat": "^4.1.0",
		"ismobilejs": "^0.5.1",
		"lodash": "^4.17.11",
		"lzma": "^2.3.2",
		"moment": "^2.22.2",
		"papaparse": "^4.6.1",
		"prop-types": "^15.6.2",
		"react": "^16.5.1",
		"react-ace": "^6.2.0",
		"react-custom-scrollbars": "^4.2.1",
		"react-dom": "^16.5.1",
		"react-element-to-jsx-string": "^14.0.2",
		"react-emotion": "^9.2.12",
		"react-expand-collapse": "^0.2.0",
		"react-feather": "^1.1.3",
		"react-joyride": "^2.0.0-15",
		"react-json-editor-ajrm": "^2.5.8",
		"react-redux": "^5.0.7",
		"react-router-dom": "^4.3.1",
		"react-virtualized": "^9.21.0",
		"recharts": "^1.3.6",
		"redux": "^3.7.2",
		"redux-saga": "^0.16.0",
		"redux-thunk": "^2.3.0",
		"reselect": "^4.0.0",
		"url-parser-lite": "^0.1.0",
		"urlsafe-base64": "https://github.com/farhan687/urlsafe-base64.git"
	},
	"devDependencies": {
		"@babel/core": "^7.1.0",
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/plugin-transform-spread": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/preset-flow": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^9.0.0",
		"babel-jest": "^23.6.0",
		"babel-loader": "^8.0.2",
		"babel-plugin-direct-import": "^0.6.0-beta.1",
		"babel-plugin-emotion": "^9.2.10",
