		"Lakhan Samani <lakhan.m.samani@gmail.com> (https://github.com/lakhansamani",
		"Appbase.io <info@appbase.io> (https://github.com/appbaseio)"
	],
	"jest": {
		"testPathIgnorePatterns": [
			"<rootDir>/app/src/batteries",
			"<rootDir>/node_modules/"
		]
	},
	"license": "MIT"
}
